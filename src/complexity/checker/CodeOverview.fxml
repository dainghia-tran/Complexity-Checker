<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="complexity.checker.CodeOverviewController">
   <children>
      <Text layoutX="272.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Code Overview" AnchorPane.topAnchor="20.0">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Text>
      <Button fx:id="printButton" layoutX="270.0" layoutY="370.0" mnemonicParsing="false" onAction="#onClickPrint" prefWidth="100.0" style="-fx-background-color: linear-gradient(to bottom, #545a63 0%, #000000 51%, #545a63 100%);" text="Print" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="saveButton" layoutX="429.0" layoutY="370.0" mnemonicParsing="false" onAction="#onClickSave" prefWidth="100.0" style="-fx-background-color: linear-gradient(to bottom, #545a63 0%, #000000 51%, #545a63 100%);" text="Save" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <TableView fx:id="table" layoutX="75.0" layoutY="125.0" prefHeight="200.0" prefWidth="650.0" AnchorPane.topAnchor="125.0">
        <columns>
          <TableColumn prefWidth="50.0" text="Line no" />
          <TableColumn prefWidth="320.0" text="Program statements" />
            <TableColumn minWidth="0.0" prefWidth="40.0" text="Cs" />
            <TableColumn minWidth="0.0" prefWidth="40.0" text="Cv" />
            <TableColumn prefWidth="40.0" text="Cm" />
            <TableColumn prefWidth="40.0" text="Ci" />
            <TableColumn editable="false" prefWidth="40.0" text="Ccp" />
            <TableColumn minWidth="0.0" prefWidth="40.0" text="Ccs" />
            <TableColumn prefWidth="40.0" text="TCps" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
