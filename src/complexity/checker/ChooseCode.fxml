<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="complexity.checker.ChooseCodeController">
   <children>
      <Text fx:id="headerName" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Header" textAlignment="CENTER" wrappingWidth="800.0" AnchorPane.topAnchor="20.0">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Text>
      <Text layoutX="308.0" layoutY="114.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Select File" textAlignment="CENTER" AnchorPane.topAnchor="90.0">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Text>
      <TextField fx:id="path" layoutX="53.0" layoutY="119.0" prefHeight="26.0" prefWidth="350.0" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="150.0" />
      <Button fx:id="javaButton" layoutX="522.0" layoutY="150.0" mnemonicParsing="false" onAction="#chooseJavaFile" prefWidth="50.0" text="JAVA" AnchorPane.topAnchor="150.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="cppButton" layoutX="591.0" layoutY="150.0" mnemonicParsing="false" onAction="#chooseCPPFile" prefWidth="50.0" text="C++" AnchorPane.topAnchor="150.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <TableView fx:id="table" fixedCellSize="0.0" layoutX="154.0" layoutY="159.0" prefHeight="131.0" prefWidth="492.0" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="158.0" AnchorPane.topAnchor="200.0">
        <columns>
          <TableColumn editable="false" prefWidth="50.0" text="Line No" />
          <TableColumn prefWidth="200.0" text="Program Statement" />
            <TableColumn prefWidth="40.0" text="Nkw" />
            <TableColumn prefWidth="40.0" text="Nid" />
            <TableColumn prefWidth="40.0" text="Nop" />
            <TableColumn prefWidth="40.0" text="Nnv" />
            <TableColumn prefWidth="40.0" text="Nsl" />
            <TableColumn prefWidth="40.0" text="Cs" />
        </columns>
      </TableView>
      <Button fx:id="editWeight" layoutX="183.0" layoutY="348.0" mnemonicParsing="false" onAction="#clickOnEditWeight" prefWidth="100.0" style="-fx-background-color: linear-gradient(to bottom, #545a63 0%, #000000 51%, #545a63 100%);" text="Edit Weight" textFill="WHITE" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="348.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="home" layoutX="285.0" layoutY="348.0" mnemonicParsing="false" onAction="#clickOnHome" prefWidth="100.0" style="-fx-background-color: linear-gradient(to bottom, #545a63 0%, #000000 51%, #545a63 100%);" text="Home" textFill="WHITE" AnchorPane.leftAnchor="280.0" AnchorPane.topAnchor="348.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="addCode" layoutX="414.0" layoutY="348.0" mnemonicParsing="false" onAction="#clickOnAddCode" prefWidth="100.0" style="-fx-background-color: linear-gradient(to bottom, #545a63 0%, #000000 51%, #545a63 100%);" text="Add Code" textFill="WHITE" AnchorPane.rightAnchor="286.0" AnchorPane.topAnchor="348.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="save" layoutX="562.0" layoutY="348.0" mnemonicParsing="false" onAction="#clickOnSave" prefWidth="100.0" style="-fx-background-color: linear-gradient(to bottom, #545a63 0%, #000000 51%, #545a63 100%);" text="Save" textFill="WHITE" AnchorPane.rightAnchor="158.0" AnchorPane.topAnchor="348.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
