<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="complexity.checker.ChooseCodeController">
   <children>
      <Text fx:id="headerName" layoutX="379.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Header" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Text layoutX="371.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Select File" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextField fx:id="path" layoutX="297.0" layoutY="120.0" />
      <Button fx:id="javaButton" layoutX="489.0" layoutY="120.0" mnemonicParsing="false" onAction="#chooseJavaFile" text="JAVA" />
      <Button fx:id="cppButton" layoutX="564.0" layoutY="120.0" mnemonicParsing="false" onAction="#chooseCPPFile" text="C++" />
      <TableView fx:id="table" layoutX="225.0" layoutY="159.0" prefHeight="131.0" prefWidth="394.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="editWeight" layoutX="231.0" layoutY="325.0" mnemonicParsing="false" onAction="#clickOnEditWeight" text="Edit Weight" />
      <Button fx:id="home" layoutX="347.0" layoutY="325.0" mnemonicParsing="false" onAction="#clickOnHome" text="Home" />
      <Button fx:id="addCode" layoutX="463.0" layoutY="325.0" mnemonicParsing="false" onAction="#clickOnAddCode" text="Add Code" />
      <Button fx:id="save" layoutX="578.0" layoutY="325.0" mnemonicParsing="false" onAction="#clickOnSave" text="Save" />
   </children>
</AnchorPane>
